/** portal::index **/

.main img {
  width: 100%;
  vertical-align: top;
}
.section h3.ys_title {
  padding: 8px 0;
  font-size: 18px;
  color: #747F8D;
  border-bottom: 1px solid #DFDFDF;
  display: block;
}
.content {
	float: left;
	width: 896px;
}
.focus-wrap {
	margin-bottom: 18px
}
.focus-wrap .focus-img {
	float: left;
	width: 400px;
	margin-right: 3%
}
.focus-wrap .focus-img img{
	width: 400px;
	height: 
}
.focus-wrap .slider {
	position: relative
}
.focus-wrap .slider .switch-nav {
	position: absolute;
	bottom: 10px;
	left: 0;
	width: 100%;
	text-align: center
}
.focus-wrap .slider .switch-nav li {
	display: inline-block;
	zoom: 1;
	*display: inline;
	margin: 0 3px
}
.focus-wrap .slider .switch-nav li a {
	display: inline-block;
	zoom: 1;
	*display: inline;
	height: 17px;
	width: 17px;
	text-indent: -999em;
	overflow: hidden;
	background: #999;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%
}
.focus-wrap .slider .switch-nav li a: hover {
	background: #fff
}

.focus-wrap .slider .switch-nav li.current a{
	background-color: white;
}

.focus-wrap .focus-topic {
	position: relative;
	overflow: hidden
}
.focus-wrap .focus-topic h3 a {
	font-size: 18px;
	color: #333
}
.focus-wrap .focus-topic h3 a:hover {
	color: #00a2ca;
	text-decoration: underline
}
.focus-wrap .focus-topic p {
	padding: 10px 0 25px;
	color: #999
}
.focus-wrap .focus-topic .relative-news-list li {
	padding: 4px 0 4px 8px
}
