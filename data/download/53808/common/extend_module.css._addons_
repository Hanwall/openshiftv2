	display: block;
	padding: 8px 0;
	font-size: 16px;
	color: #fff;
	text-align: center;
	background: #00a2ca;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px
}
.post-subject .post-btn {
  display: block;
  padding: 8px 0;
  font-size: 16px;
  color: #FFF;
  text-align: center;
  background: #00A2CA;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
}
.post-subject .post-btn span {
	display: inline-block;
	*zoom: 1;
	padding-left: 35px;
	line-height: 28px;
	cursor: pointer;
	background: url({STYLEIMGDIR}/sprite.png) no-repeat 0 -198px
}
.login-info-wrap {
	padding: 5px 12px;
	margin-bottom: 15px;
	border: 2px solid #eee;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px
}
.login-info-wrap dl {
	color: #666
}
.login-info-wrap dl dt,.login-info-wrap dl dd {
	float: left;
	line-height: 31px
}
.login-info-wrap dl dd.num {
	padding-right: 20px;
	font-family: Georgia;
	font-size: 18px;
	line-height: 27px
}
.login-info-wrap dl dd a {
	color: #333
}
.login-info-wrap ul li {
	float: left;
	padding: 6px 30px 6px 10px;
	background: url({STYLEIMGDIR}/dot.gif) no-repeat 0 12px
}
.login-info-wrap ul li a: hover {
	color: #00a2ca
}
.top-category {
	margin-bottom: 30px
}
.top-category .con {
	width: 97%;
	padding: 30px 0 0
}
.top-category .box {
	float: left;
	width: 46%;
	min-height: 425px
}
.top-category .box h2 {
	padding: 0 0 15px
}
.top-category .box h2 a {
	color: #00a2ca;
	font-size: 16px
}
.top-category .box h3 {
	padding: 10px 0;
	display: block;
}
.top-category .box h3 a {
	font-size: 18px
}
.top-category .box h3 a:hover {
	color: #00a2ca;
	text-decoration: underline
}
.top-category .box p {
	padding: 0 0 15px;
	color: #999
}
.top-category .box .info {
	margin-bottom: 20px
}
.top-category .box .info span {
	display: inline;
	padding: 0 0 0 20px;
	color: #666;
	background: url({STYLEIMGDIR}/sprite.png) no-repeat 0 -40px
}
.top-category .box .info .underline-time {
	float: left;
	color: #999;
	background-position: 0 -39px
}
.top-category .box .info .watched-num {
	float: right;
	padding-left: 25px;
	background-position: 0 -77px
}
.top-category .box .info .comment-num {
	float: right;
	background-position: 0 -116px;
	margin-left: 15px
}
.top-category .even {
	padding-right: 3.8%;
	border-right: 1px dashed #dfdfdf
}
.top-category .odd {
	padding-left: 4%
}
.top-category .relative-news-list {
	padding: 24px 0 0;
	border-top: 1px dashed #e9e9e9
}
.top-category .first-box,.top-category .second-box {
	padding-bottom: 30px;
	border-bottom: 1px dashed #dfdfdf
}
.top-category .third-box,.top-category .fourth-box {
	padding-top: 30px
}
.special-column {
	margin-bottom: 30px
}
.special-column .column-item {
	float: left;
	width: 22%;
	padding-right: 3%
}
.special-column .column-item h3 {
	padding: 15px 0;
	display: block;
}
.special-column .column-item h3 a {
	font-size: 16px;
	color: #333
}
.special-column .column-item .relative-news-list {
	padding: 15px 0 20px
}
.special-column .column-item .enter-btn {
	display: inline-block;
	*zoom: 1;
	padding: 3px 25px;
	color: #fff;
	font-size: 12px;
	background: #00a2ca;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px
}
.sidebar-index {
	float: right;
	width: 280px
}
.theme-wrap ul.top-theme-list li {
	padding: 0 0 23px;
	overflow:hidden;
}
.theme-wrap ul.top-theme-list li .pic {
	float: left;
	margin-right: 10px
}
.theme-wrap ul.top-theme-list li .txt {
	position: relative;
	overflow: hidden
}
.theme-wrap ul.top-theme-list li .txt a:hover {
	color: #00a2ca
}
.theme-wrap ul.top-theme-list li .txt h3 a{
	padding: 0 0 5px;
	line-height: 14px;
	font-size: 12px;
	font-weight: bold;
}
.theme-wrap ul.top-theme-list li .txt p {
	color: #999;
	margin-top: 5px;
}
.hot-plate {
	margin-bottom: 20px;
	border-top: 4px solid #00a2ca
}
.hot-plate h3.ys_title {
	padding: 8px 15px;
	border: 1px solid #e9e9e9
}
.hot-plate ul {
	position: relative;
	height: 100%;
	overflow: hidden;
	
}

.hot-plate ul li {
	width: 139px;
	float: left;
	border-left: 1px solid #e9e9e9
}
.hot-plate ul li a {
	display: block;
	padding: 10px 0 10px 15px;
	font-size: 14px;
	border-right: 1px solid #e9e9e9;
	border-bottom: 1px solid #e9e9e9
}

.ie_all .hot-plate ul li {}
.hot-plate ul li a: hover {
	color: #fff;
	background: #00a2ca
}
.hot-plate .more {
	display: block;
	padding: 10px 0;
	text-align: center;
	font-size: 14px;
	border: 1px solid #e9e9e9;
	border-top: none
}
.hot-plate .more: hover {
	color: #00a2ca
}
.yun-weibo .con {
	padding: 15px 0px;
}

.yun-weibo p {
	padding: 5px 0 10px;
	color: #666;
	background-color: #FAFAFA;

}
.yun-weibo ul.hot-list li {
	padding: 2px 0 2px 18px;
	font-size: 12px;
	color: #666;
	background: url({STYLEIMGDIR}/sprite.png) no-repeat 0 -158px
}
.yun-weibo ul.hot-list li a {
	font-size: 14px;
	color: #333
}
.yun-weibo ul.hot-list li a: hover {
	color: #00a2ca
}
.weeky-daren {
	margin-bottom: 20px
}
.weeky-daren a: hover {
	color: #00a2ca
}
.weeky-daren h3.ys_title {
	border-bottom: none
}
.weeky-daren .con {
	border: 1px solid #eee;
	background: #fbfbfb;
	padding: 0 14px
}
.weeky-daren dl.user-list-r {
	position: relative;
	width: 100%;
	overflow: hidden;
	padding-bottom: 12px;
	border-bottom: 1px solid #e9e9e9
}
.weeky-daren dl.user-list-r dt {
	padding: 10px 0;
	font-size: 14px;
	color: #666
}

.weeky-daren dl.user-list-r dd ul li {
	float: left;
	display: inline;
	margin-right: 35px;
	width: 48px;
	height: 90px;
	overflow: hidden
}


.weeky-daren dl.user-list-r dd span {
	display: block;
	color: #999
}
.weeky-daren .user-list-r.last {
	border-bottom: none
}
.latest-post-wrap .tb {
	border-bottom: 1px solid #ddd;
	background:none;
	
}
.latest-post-wrap .frame-tab .title .move-span{ width:30% !important;}
.latest-post-wrap .tab-title{ background:none;}
.latest-post-wrap .tb li {
	position: relative;
	float: left;
	background: #fff
	border:none;
}
.latest-post-wrap .tb li a {
	display: block;
	float: left;
	cursor: pointer;
	font-size:14px;
	line-height:30px;
	border:none;
}
.latest-post-wrap .tb li.a {
	top: 1px;
	border: 1px solid #ddd;
	border-bottom: none
}
.latest-post-wrap .tb li.a a {
	font-weight: 700
}
.latest-post-wrap .tb-c {
	padding: 15px 0
}
.latest-post-wrap .panel ul li {
	position: relative;
	overflow: hidden;
	height: 24px;
	white-space: nowrap;
	text-overflow: ellipsis;
	line-height: 24px
}
.latest-post-wrap .panel ul li a {
	font-size: 13px
}
.latest-post-wrap .panel ul li a: hover {
	color: #00a2ca;
	text-decoration: underline
}


.friendlinks .con {
	padding: 15px 0 30px
}
.friendlinks a {
	padding: 0 16px 0 0;
	font-size: 14px;
	line-height: 24px
}
.friendlinks a: hover {
	color: #00a2ca
}
.relative-news-list li {
	padding: 3px 0 3px 15px;
	background: url({STYLEIMGDIR}/dot.gif) no-repeat 0 11px
}
.relative-news-list li a {
	font-size: 14px;
	line-height: 16px
}
.relative-news-list li a: hover {
	color: #00a2ca;
	text-decoration: underline
}

.special-column {
  margin-bottom: 30px;
}

