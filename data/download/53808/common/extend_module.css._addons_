/* CSS Document */
/** portal::index,portal::list,portal::view,forum::index,forum::forumdisplay **/

.frame, .frame-tab{margin-bottom:0 !important;border:0 !important;background:none !important;}
.block{ margin:0 !important;}

/** end **/
/** forum::index **/

.bm_h{ height:41px;}
.fl.bm{ background:none; border:none;}
#online.bm{ background:none; border-color:#e5e5e5;}
.fl .bm,#online.bm,#category_lk {
	margin-bottom: 15px;
	-webkit-border-top-right-radius: 3px;
	-moz-border-radius-topright: 3px;
	border-top-right-radius: 3px;
	-webkit-border-top-left-radius: 3px;
	-moz-border-radius-topleft: 3px;
	border-top-left-radius: 3px;
	border: 1px solid #e5e5e5
}
.bm .bm_h {
	position: relative;
	
	-webkit-border-top-right-radius: 3px;
	-moz-border-radius-topright: 3px;
	border-top-right-radius: 3px;
	-webkit-border-top-left-radius: 3px;
	-moz-border-radius-topleft: 3px;
	border-top-left-radius: 3px;
	border-bottom: 1px solid #efefef;
	border-top:0;
	background: url({STYLEIMGDIR}/tbg.gif) repeat-x 0 0
}
.bm .bm_h h2,.bm_h h3{ font-size: 14px;color: #666; line-height:41px;}
.fl .bm_h  h2 a,.bm_h h3 a {
	color: #666;
	font-weight:200;
}
.fl .bm .bm_c h2 a,.fl .bm .bm_c dt a{ font-size:16px;}
.fl .bm .bm_c .fl_i,.fl .bm .bm_c .fl_by,.fl .bm .bm_c dd { color:#999;}
/** end **/

/** forum::forumdisplay,forum::guide **/
.bm_h{ padding:0;}
.forum_info p{ line-height:22px; color:#999;}
.forum_info .moderator{}
.forum_info .forum_count{}
.forum_info .forum_rule{ line-height:22px;}
.tl .th{ background:#FBFBFB;}

.tl tr:hover th,.tl tr:hover td {
    background-color: #FBFBFB
}



.tabA {
  border-bottom: 1px solid #E5E5E5;
  height: 28px;
  padding: 10px 10px 10px 0;
}
.tabA ul {
  _position: absolute;
  font-size: 14px;
  overflow: hidden;
  background: #FFF;
}
.tabA li {
  float: left;
  margin-left: -1px;
  margin-top: 5px;
}
.tabA li a {
  float: left;
  color: #00A2CA;
  line-height: 1.2;
  padding: 0 15px;
  overflow: hidden;
  border: none;
  padding: 0 10px;
  font-size: 12px;
}
.tabA li.current {
  margin: 0;
  margin-right: 6px;
}
.tabA li.current a {
  color: #FFF;
  font-weight: 700;
  background: #00A2CA;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  line-height: 24px;
}

.ttp{ border-width:0;}

.ttp a, .ttp strong{ border-color:#dfdfdf;}
.ttp .a a{ border-color:#dedede; color:#555;}
.ttp a:hover{border-color:#00A2CA;	}



/** end **/
/** forum::viewthread **/
.ad .plc{  background: #E5E5E5;}
.ad .pls{ height:2px;}
.pgbtn .bm_h{ background-color:#fefefe;}
.pgbtn a{ border: 1px solid #DFDFDF;}
.pgbtn a:hover { border-color: #00A2CA; text-decoration: none; }
.pgbtn a:active { background: #00A2CA; border-color: #FFF; box-shadow: none; }


.pls .avatar img {
	border-radius:5px;
}
/** end **/
